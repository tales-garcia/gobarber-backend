  "scripts": {
    "build": "npm run js:build && npm run js:bundle && npm run js:minify",
    "js:build": "babel src --ignore \"src/modules/**/services/*.spec.ts\" --extensions \".js,.ts\" --out-dir tmp/javascript --copy-files",
    "js:bundle": "browserify --transform hbsfy --no-bundle-external --entry tmp/javascript/shared/infra/http/server.js --outfile ./dist/bundle.js",
    "js:minify": "terser dist/bundle.js --compress --mangle --output ./dist/bundle.js",
    "prod:server": "node dist/bundle.js",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "commit": "cz",
    "test": "jest"
  },
